<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
                                         "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
 <session-factory name="">
  <property name="hibernate.connection.driver_class">org.postgresql.Driver</property>
  <property name="hibernate.connection.password">mworkpass</property>
  <property name="hibernate.connection.url">jdbc:postgresql:mwork</property>
  <property name="hibernate.connection.username">varrek</property>
  <property name="hibernate.dialect">org.hibernate.dialect.PostgreSQLDialect</property>
  <property name="hibernate.show_sql">true</property>
  <property name="hibernate.session_factory_name">org.varrek.mwork.HibernateUtil</property>
  <mapping class="org.varrek.mwork.user.User"/>
  <mapping class="org.varrek.mwork.user.UserGroup"/>
  <mapping class="org.varrek.mwork.repo.Repo"/>
  <mapping class="org.varrek.mwork.repo.RepoAccess"/>
  <mapping class="org.varrek.mwork.repo.Keys"/>
  <mapping class="org.varrek.mwork.files.AbsctactFile"/>
  <mapping class="org.varrek.mwork.files.Folder"/>
  <mapping class="org.varrek.mwork.files.File"/>
 </session-factory>
</hibernate-configuration>
